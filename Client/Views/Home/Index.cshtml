<!-- Banner -->
<section class="banner banner-01 bg-holder bg-overlay-black-80" style="background-image: url(dist/images/bg/01.jpg);">
    <div class="container">
        <div class="row justify-content-center position-relative">
            <div class="col-lg-10 text-center">
                <h1 class="text-white mb-3">Setup up great events</h1>
                <p class="text-white banner-sub-title mb-5">Get Ready for The Next Event, Buy Tickets Now And Stay With Us</p>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <form class="home-search">
                    <div class="state-search">
                        <div class="form-group mb-3 mb-lg-0">
                            <span>What?</span>
                            <input type="text" class="form-control" placeholder="Ex: events name">
                        </div>
                    </div>
                    <div class="state-submit">
                        <a class="btn btn-primary" href="#"> <i class="fas fa-search-location"></i> Search </a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

